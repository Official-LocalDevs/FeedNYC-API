      {
        "_id": "660a91248747cd35103e54ba",
        "name": "Al Hajjah Fajr Tyson Halal Food and Clothing Distribution Pantry",
        "address": "2269 Crotona Ave, Bronx, NY, 10457",
        "contact": "(646) 353-0859",
        "tags": ["Halal"],
        "hours": {
          "food_pantry": {
